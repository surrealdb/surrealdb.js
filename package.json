{
	"name": "surrealdb.js",
	"version": "1.3.2",
	"private": true,
	"packageManager": "^bun@1.1.17",
	"workspaces": ["packages/*"],
	"devDependencies": {
		"@biomejs/biome": "1.8.3"
	},
	"scripts": {
		"qc": "biome check .",
		"qa": "biome check . --write",
		"qau": "biome check . --write --unsafe",
		"build:cbor": "bun run -F @surrealdb/cbor build",
		"build:sdk": "bun run -F @surrealdb/sdk build"
	},
	"browser": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.mjs",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts",
			"browser": "./dist/index.mjs"
		}
	},
	"files": ["dist"]
}
