  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "preversion": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/surrealdb/surrealdb.js.git"
  },
  "author": {
