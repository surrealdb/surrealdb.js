{
	"name": "surrealdb.js",
	"version": "1.3.2",
	"private": true,
	"packageManager": "^bun@1.1.17",
	"workspaces": ["packages/*"],
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"bun-types": "^1.2.12"
	},
	"scripts": {
		"qc": "biome check .",
		"qa": "biome check . --write",
		"qau": "biome check . --write --unsafe",
		"build:cbor": "bun run -F @surrealdb/cbor build",
		"build:sdk": "bun run -F surrealdb build",
		"test": "bun run build:cbor && bun run build:sdk && cd ./packages/tests && bun run test"
	}
}
