{
	"name": "surrealdb.js",
	"version": "1.3.2",
	"private": true,
	"packageManager": "^bun@1.1.17",
	"workspaces": ["packages/*"],
	"devDependencies": {
		"@biomejs/biome": "1.8.3"
	},
	"scripts": {
		"qc": "biome check .",
		"qa": "biome check . --write",
		"qau": "biome check . --write --unsafe",
		"build:cbor": "bun run -F @surrealdb/cbor build",
		"build:sdk": "bun run -F @surrealdb/sdk build",
		"build:legacy": "bun run -F @surrealdb/legacy build"
	}
}
