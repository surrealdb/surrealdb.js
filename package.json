  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@types/ws": "^8.5.3",
    "rollup": "^2.79.0",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.8.3"
  },
