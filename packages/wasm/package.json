{
    "name": "@surrealdb/wasm",
    "version": "3.0.0",
    "surreal": "3.0.0",
    "type": "module",
    "license": "Apache-2.0",
    "description": "Emeddable WebAssembly browser engine for SurrealDB",
    "homepage": "https://github.com/surrealdb/surrealdb.js",
    "keywords": [
        "surrealdb",
        "surrealdb.js",
        "wasm",
        "browser",
        "embedded"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/surrealdb/surrealdb.js.git"
    },
    "scripts": {
        "build": "bun run build.ts",
        "sync": "bun run ../../scripts/sync-surreal.ts",
        "deploy": "bun run ../../scripts/publish.ts"
    },
    "peerDependencies": {
        "surrealdb": "^2.0.0-0"
    },
    "devDependencies": {
        "surrealdb": "^2.0.0-0"
    },
    "exports": {
        ".": {
            "import": "./dist/surrealdb-wasm.mjs",
            "types": "./dist/surrealdb-wasm.d.ts"
        },
        "./worker": {
            "import": "./dist/worker-agent.mjs",
            "types": "./dist/worker-agent.d.ts"
        }
    },
    "files": [
        "dist",
        "wasm/*.wasm"
    ]
}
