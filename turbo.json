{
    "$schema": "https://turborepo.org/schema.json",
    "globalPassThroughEnv": ["ACTIONS_ID_TOKEN_REQUEST_URL", "ACTIONS_ID_TOKEN_REQUEST_TOKEN"],
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "target/**", "wasm/**"]
        },
        "run-tests": {
            "cache": false
        },
        "sync": {
            "cache": false
        },
        "deploy": {
            "cache": false
        }
    }
}
